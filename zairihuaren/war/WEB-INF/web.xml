<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Zairihuaren</servlet-name>
		<servlet-class>com.zairihuaren.ZairihuarenServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Zairihuaren</servlet-name>
		<url-pattern>/zairihuaren</url-pattern>
	</servlet-mapping>
		<servlet>
		<servlet-name>RateInfo</servlet-name>
		<servlet-class>com.zairihuaren.RateInfoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RateInfo</servlet-name>
		<url-pattern>/rate_info</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
